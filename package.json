  "scripts": {
    "dev": "farmup -w",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "mikro-orm:migration": "mikro-orm migration:create",
    "mikro-orm:migrate": "mikro-orm migration:up",
    "mikro-orm:generate": "mikro-orm schema:update --run"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.0",
    "@golevelup/nestjs-webhooks": "^0.2.19",
    "@mikro-orm/cli": "^6.4.3",
    "@mikro-orm/core": "^6.4.3",
    "@mikro-orm/migrations": "^6.4.3",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/postgresql": "^6.4.3",
    "@mikro-orm/reflection": "^6.4.3",
    "@mikro-orm/seeder": "^6.4.3",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.3",
    "@nestjs/cqrs": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.4",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.0.3",
    "@nestjs/throttler": "^6.3.0",
    "@prisma/client": "6.2.1",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.3.2",
    "cache-manager-ioredis-yet": "^2.1.2",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^5.2.1",
    "helper-fns": "^2.9.2",
    "ioredis": "^5.4.2",
    "moment": "^2.30.1",
    "nestjs-pino": "^4.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "farmup": "latest",
    "jest": "29.7.0",
    "prettier": "^3.4.2",
    "prisma": "6.2.1",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "configPaths": [
      "./src/database/mikro-orm-cli.config.ts",
      "./dist/mikro-orm-cli.config.js"
    ]
  }
